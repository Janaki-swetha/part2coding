class Solution:
    def findPairs(self, nums: List[int], k: int) -> int:
        result=set()
        n=len(nums)
        for i in range(0,n-1):
            for j in range(i+1,n):
                if nums[j]-nums[i]==k:
                    result.add((nums[i],nums[j]))
                if nums[i]-nums[j]==k:
                    result.add((nums[j],nums[i]))  
        return len(result)   
