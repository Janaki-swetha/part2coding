class NumArray:

    def __init__(self,arr):
        n=len(arr)
        self.pf=[0]*(n+1)
        self.pf[0]=arr[0]
        for i in range(1,n):
            self.pf[i]=self.pf[i-1]+arr[i]

        

    def sumRange(self,l,r):
        return self.pf[r]-self.pf[l-1]
        
